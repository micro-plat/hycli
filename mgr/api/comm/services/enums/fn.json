{-{- $rp:="rp"}-}
{-{- $split:="/:"}-}
{-{- $end:="?"}-}
{-{- $max := (len $tables)|minus}-}
[
{-{- range $i,$c:=$tables}-}
{
    "name":"{-{$c.Desc}-}",
    "url": "/{-{$c.Name.MainPath}-}{-{flterJoinColumnNames $c $rp  $split $end}-}",
    "table":"{-{$c.Name.Raw}-}",
    "path":"../views/{-{$c.Name.Prefix}-}/{-{$c.Name.Main}-}/{-{$c.Name}-}.list.vue",
    "funcs":[
        {-{- $opts := mergeOptrs $c.ListOpts $c.BarOpts $c.ViewOpts $c.ViewExtCmptOpts.ALL $c.LStatOpts $c.ChartOpts $c.ExtOpts $c.QueryOptrs}-}
        {-{- $vax := (len $opts)|minus}-}
        {-{- range $j,$k := $opts}-}
        {"id":"{-{$k.UNQ}-}","name":"{-{$k.Label}-}","desc":"{-{$k.Desc}-}"}{-{if lt $j $vax}-},{-{end}-}
        {-{- end}-}
    ]
}{-{if lt $i $max}-},{-{end}-}
